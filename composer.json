{
    "name": "artisanry/chartist",
    "description": "Chartist Integration with a Fluent Builder for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Chartist",
        "Laravel Chartist"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Chartist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Chartist\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Chartist\\ChartistServiceProvider"
            ],
            "aliases": {
                "Chartist": "Artisanry\\Chartist\\Facades\\Chartist"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
